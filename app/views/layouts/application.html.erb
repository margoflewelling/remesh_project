<!DOCTYPE html>
<html>
  <head>
    <title>RemeshProject</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_pack_tag 'application' %>
  </head>

  <body>
    <nav class = "topnav">
      <%= link_to "All Conversations", "/"%>
    </nav>

    <% flash.each do |type, message| %>
      <div class= "<%=type%>-flash">
      <p><%= message %></p>
      </div>
    <% end %>
    
    <%= yield %>
  </body>
</html>
